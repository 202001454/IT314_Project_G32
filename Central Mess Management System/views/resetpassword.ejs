    <div id="wrapper">
        <%- include('../partials/nav_home_manager') %>

        
        
    <div class="container bg-dark mt-3 col-md-6 col-xl-4 px-5 py-4 mb-5 my-5 rounded-5">
        <div class="text-center fs-2 m-4 text-light">Reset Password</div>
        

        <form action="#" method="post">
            <div class="form-floating my-4">
                <input type="password" class="form-control" id="Password" name="password" placeholder="Password">
                <label for="Password">Password</label>
            </div>
            <div class="form-floating my-4">
                <input type="password" class="form-control" id="ConfirmPassword" name="cpassword" placeholder="Password">
                <label for="ConfirmPassword">Confirm Password</label>
            </div>

            <div class="text-center my-4">
                <button class="btn btn-primary mt-4 col-3" type="submit">Submit</button>
            </div>
        </form>
    </div>

