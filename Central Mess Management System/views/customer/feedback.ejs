<%- include('../partials/header_customer') %>


  <div id="wrapper">

    <%- include('../partials/nav_home_customer') %>


      <div class="container">


        <div class="row mb-5">
          <div class="col-12 col-lg-3">
            <div class="list-group text-center make-sticky ">
        
                <table class="table table-hover">
                    <thead>
                        <tr>
                            <div class="profile my-4 table-striped-columns">
                                <div class="mb-3"><img src="/photos/person_logo.svg" alt="person"
                                        width="20%"></div>
                                <div class="fs-3"><%= customer.fullname%></div>
                                <div class="fs-5">Customer</div>
                            </div>
                        </tr>
                    </thead>
                    <tbody>
                        <tr>
                            <td><a href="/customer/<%= customer.username%>"
                                    class="list-group-item list-group-item-action">Home</a></td>
                        </tr>
                        <tr>
                            <td><a href="/customer/<%= customer.username%>/view"
                                    class="list-group-item list-group-item-action">View
                                    profile</a></td>
                        </tr>
                        <tr >
                            <td><a href="/customer/<%= customer.username%>/edit"
                                    class="list-group-item list-group-item-action">Edit
                                    Profile</a></td>
                        </tr>
                        <tr>
                            <td><a href="/customer/<%= customer.username%>/changepassword"
                                    class="list-group-item list-group-item-action">Change
                                    password</a></td>
                        </tr>
        
                        <tr>
                            <td><a href="/customer/<%= customer.username%>/paymenthistory"
                                    class="list-group-item list-group-item-action">Payment History</a></td>
                        </tr>
                        <tr class="table-active">
                            <td><a href="/customer/<%= customer.username%>/feedback"
                                    class="list-group-item list-group-item-action">Feedback</a></td>
                        </tr>
                    </tbody>
                </table>
        
        
        
            </div>
        </div>

                    <div class="col-1 col-lg-0"></div>
                    <div class="col-12 col-lg-6 mt-5">

                        <div class="container border rounded-5">
                            <div class="fs-3 py-2 text-center mt-4">We value your Feedback.</div>
                            <div class=" fs-4 pb-2 text-center">Give your opinion.</div>

                            <form class="mt-4 ms-5">
                                <div class="fs-5 py-2">Cleanliness</div>
                                <div class="rating  d-flex flex-row  mb-2 ">
                                    <div class="form-check">
                                        <input class="form-check-input" type="radio" name="cleanliness" value="1" id="rating1">
                                        <label class="form-check-label form-check-inline" for="rating1">
                                            1
                                        </label>
                                    </div>
                                    <div class="form-check">
                                        <input class="form-check-input form-check-inline" type="radio"
                                            name="cleanliness" value="2" id="rating2">
                                        <label class="form-check-label form-check-inline" for="rating2">
                                            2
                                        </label>
                                    </div>
                                    <div class="form-check">
                                        <input class="form-check-input" type="radio" name="cleanliness" value="3" id="rating3">
                                        <label class="form-check-label form-check-inline" for="rating3">
                                            3
                                        </label>
                                    </div>
                                    <div class="form-check">
                                        <input class="form-check-input" type="radio" name="cleanliness" value="4" id="rating4">
                                        <label class="form-check-label form-check-inline" for="rating4">
                                            4
                                        </label>
                                    </div>
                                    <div class="form-check">
                                        <input class="form-check-input" type="radio" name="cleanliness" value="5" id="rating5">
                                        <label class="form-check-label" for="rating5">
                                            5
                                        </label>
                                    </div>
                                </div>
                                <div class="fs-5 py-2">Food Quality</div>
                                <div class="rating  d-flex flex-row  mb-2 ">
                                    <div class="form-check">
                                        <input class="form-check-input" type="radio" name="food" value="1" id="rating1">
                                        <label class="form-check-label form-check-inline" for="rating1">
                                            1
                                        </label>
                                    </div>
                                    <div class="form-check">
                                        <input class="form-check-input form-check-inline" type="radio"
                                        name="food" value="2" id="rating2">
                                        <label class="form-check-label form-check-inline" for="rating2">
                                            2
                                        </label>
                                    </div>
                                    <div class="form-check">
                                        <input class="form-check-input" type="radio" name="food" value="3" id="rating3">
                                        <label class="form-check-label form-check-inline" for="rating3">
                                            3
                                        </label>
                                    </div>
                                    <div class="form-check">
                                        <input class="form-check-input" type="radio" name="food" value="4" id="rating4">
                                        <label class="form-check-label form-check-inline" for="rating4">
                                            4
                                        </label>
                                    </div>
                                    <div class="form-check">
                                        <input class="form-check-input" type="radio" name="food" value="5" id="rating5">
                                        <label class="form-check-label" for="rating5">
                                            5
                                        </label>
                                    </div>
                                </div>
                                <div class="fs-5 py-2">Service</div>
                                <div class="rating  d-flex flex-row  mb-5 ">
                                    <div class="form-check">
                                        <input class="form-check-input" type="radio" name="service" value="1" id="rating1">
                                        <label class="form-check-label form-check-inline" for="rating1">
                                            1
                                        </label>
                                    </div>
                                    <div class="form-check">
                                        <input class="form-check-input form-check-inline" type="radio"
                                        name="service" value="2" id="rating2">
                                        <label class="form-check-label form-check-inline" for="rating2">
                                            2
                                        </label>
                                    </div>
                                    <div class="form-check">
                                        <input class="form-check-input" type="radio" name="service" value="3" id="rating3">
                                        <label class="form-check-label form-check-inline" for="rating3">
                                            3
                                        </label>
                                    </div>
                                    <div class="form-check">
                                        <input class="form-check-input" type="radio" name="service" value="4" id="rating4">
                                        <label class="form-check-label form-check-inline" for="rating4">
                                            4
                                        </label>
                                    </div>
                                    <div class="form-check">
                                        <input class="form-check-input" type="radio" name="service" value="5" id="rating5">
                                        <label class="form-check-label" for="rating5">
                                            5
                                        </label>
                                    </div>
                                </div>
    
                                <div>
                                    <div class="form-group me-5">
                                        <label class="fs-5 mb-2" for="comments">Give your comments</label>
                                        <textarea class="form-control " name="comment" id="comments" rows="3"
                                            placeholder="start here ..."></textarea>
                                    </div>
                                </div>
    
    
                                <div class="text-center my-4">
                                    <button class="btn btn-primary col-3" type="submit">Submit</button>
                                </div>
    
    
                            </form>

                        </div>
                    </div>
                    <div class="col-1 col-lg-0"></div>
                </div>
            </div>
    
            <%- include('../partials/footer') %>


